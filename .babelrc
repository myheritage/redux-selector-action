{
    "presets": ["@babel/preset-env"],
    "env": {
        "commonjs": {
            "plugins": [
                ["@babel/plugin-transform-modules-commonjs", { "loose": true }]
            ]
        },
        "umd": {
            "plugins": [
                ["babel-plugin-transform-es2015-modules-umd", { "loose": true }]
            ],
            "moduleId": "ReduxSelectorAction"
        },
        "test": {
            "plugins": [
                ["@babel/plugin-transform-modules-commonjs", { "loose": true }]
            ]
        }
    }
}